{
  "name": "kysely-sqlite-builder",
  "type": "module",
  "version": "0.6.2",
  "description": "Utility layer for Kysely on SQLite",
  "author": "subframe7536",
  "license": "MIT",
  "repository": "https://github.com/subframe7536/kysely-sqlite-builder",
  "keywords": [
    "kysely",
    "sqlite",
    "database",
    "utils"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./schema": {
      "import": "./dist/schema.js",
      "require": "./dist/schema.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typesVersions": {
    "*": {
      "schema": [
        "./dist/schema.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest --run",
    "test:bench": "vitest bench",
    "release": "pnpm test && pnpm run build && bumpp --all && npm publish"
  },
  "peerDependencies": {
    "kysely": "^0.27.3"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.5.13",
    "@subframe7536/type-utils": "^0.1.6",
    "@types/node": "^20.11.30",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "kysely": "^0.27.3",
    "kysely-wasm": "^0.6.0",
    "node-sqlite3-wasm": "^0.8.13",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0"
  }
}
