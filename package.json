{
  "name": "kysely-sqlite-builder",
  "type": "module",
  "version": "0.8.6",
  "description": "Utility layer for Kysely on SQLite",
  "author": "subframe7536",
  "license": "MIT",
  "repository": "https://github.com/subframe7536/kysely-sqlite-builder",
  "keywords": [
    "kysely",
    "sqlite",
    "database",
    "utils"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./schema": {
      "import": "./dist/schema.js",
      "require": "./dist/schema.cjs"
    },
    "./migrator": {
      "import": "./dist/migrator.js",
      "require": "./dist/migrator.cjs"
    },
    "./plugin": {
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typesVersions": {
    "*": {
      "schema": [
        "./dist/schema.d.ts"
      ],
      "migrator": [
        "./dist/migrator.d.ts"
      ],
      "plugin": [
        "./dist/plugin.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "test": "bun test",
    "release": "bun test && bun run build && bumpp --all && npm publish"
  },
  "peerDependencies": {
    "kysely": "^0.27.3",
    "magic-string-stack": "^0.1.1",
    "unplugin": "^1.10.1"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.6.6",
    "@subframe7536/type-utils": "^0.1.6",
    "@types/bun": "^1.1.6",
    "bumpp": "^9.4.1",
    "eslint": "^9.6.0",
    "kysely": "^0.27.4",
    "kysely-wasm": "^0.6.1",
    "magic-string-stack": "^0.1.1",
    "node-sqlite3-wasm": "^0.8.17",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "unplugin": "^1.11.0"
  }
}
